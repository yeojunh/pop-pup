{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ], 
    "functions": {
        "backend/api/scraper.py": {
            "runtime": "@vercel/python",
            "memory": 1024, 
            "maxDuration": 60
        }
    }
}
